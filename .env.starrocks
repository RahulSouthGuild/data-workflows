# StarRocks Connection Configuration
# Use these credentials to connect to your StarRocks cluster

# StarRocks Host (use localhost for host machine, starrocks-fe for Docker network)
STARROCKS_HOST=localhost
STARROCKS_HOST_DOCKER=starrocks-fe

# StarRocks MySQL Port
STARROCKS_PORT=9030

# Database Name
STARROCKS_DATABASE=datawiz

# Admin User Credentials (created automatically on first startup)
STARROCKS_USER=datawiz_admin
STARROCKS_PASSWORD=0jqhC3X541tP1RmR.5

# Root Credentials (for administrative tasks)
STARROCKS_ROOT_USER=root
STARROCKS_ROOT_PASSWORD=

# Connection URLs
# From host machine:
# mysql -h 127.0.0.1 -P 9030 -u datawiz_admin -p'0jqhC3X541tP1RmR.5' datawiz

# From Docker container:
# mysql -h starrocks-fe -P 9030 -u datawiz_admin -p'0jqhC3X541tP1RmR.5' datawiz

